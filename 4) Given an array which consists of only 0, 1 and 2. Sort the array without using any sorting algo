import java.util.*;
import java.lang.*;
import java.io.*;

// The main method must be in a class named "Main".
class Main {
    public static void main(String[] args) {
        // int[] ans = new int[6];
        
        int[] arr = {0,1,2,1,2,0};
        zot(arr);
        
        
    }
    public static void zot(int[] arr){
        int[] ans = new int[6];
        int zeros = 0;
        int ones = 0;
        int twos = 0;
        
        for(int i = 0; i<arr.length;i++){
            if(arr[i] == 0){
                zeros++;
            }
            if(arr[i] == 1){
                ones++;
            }
            if(arr[i] == 2){
                twos++;
            }
        }
        int count = 0;
        for(int j = count;j<zeros;j++){
            ans[j] = 0;
            count++;
        }
        for(int k = count;k<zeros+ones;k++){
            ans[k] = 1;
            count++;
        }
        for(int l = count;l<zeros + ones + twos;l++){
            ans[l] = 2;
            count++;
        }
        
        for(int p = 0; p<ans.length;p++){
            System.out.print(ans[p] + " ");
        }
    }
}
